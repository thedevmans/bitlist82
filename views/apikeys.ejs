         <div class="form-group">
            <label for="Select_reason">Select your bot</label>
            <select style="background-color:#35363A;color:white" class="form-control" id="Select_bot">
               <% data.forEach(function(data) { %>
               <option><%=data.val().name%></option>
               <% }); %>
            </select>
         </div>
         <a style="margin-top:30px;" id="generate" class="button">Generate Key</a>
      </div>
      <script>
         $('#generate').click(function(){ generate(); return false; });
           function generate() {
             var bot = document.getElementById("Select_bot").value
             if(!bot) {
                   Swal.fire({
                     icon: "error",
                     title: "Oops...",
                     text: "Select your bot",
                   });        
             }else {
                    $.post(
                     "/api/generate/key",
                     {
                       bot: bot,
                     },
                     function(data, status) {
                       window.location = data;
                     }
                   );    
             }
           }
      </script>

