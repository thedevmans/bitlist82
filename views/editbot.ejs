<script>

  var editor = CodeMirror.fromTextArea(document.getElementById("editor"), {
    lineNumbers: true
  });
async function edit() {
  var token = Cookies.get("token");
  fetch("https://discordapp.com/api/users/@me", {
    headers: {
      Authorization: `Bearer ${token}`
    }
  })
    .then(res => res.json())
    .then(response => {
      const { username, discriminator } = response;
      
      var owner = response.id;
      var bot = window.location.href.split("/edit/")[1];
      var tags = document.getElementById("tags").value;
      var owners = document.getElementById("bot_owners").value;
      var background = ""
      if(!document.getElementById("bot_background")) {
      background = ""
      }else {
      background = document.getElementById("bot_background").value;        
      }
      var library = document.getElementById("library").value;
      var short_description = document.getElementById("short_description")
        .value;
      var server_invite = document.getElementById("server_invite").value;
      var bot_invite = document.getElementById("bot_invite").value;
      var website = document.getElementById("website").value;
      var prefix = document.getElementById("bot_prefix").value;
      var string = editor.getValue()
      var text =   string.replace("<script>"," ").replace("</ script>"," ").replace(" ` "," ' ")
      var long_description = text
      if (long_description.length < 300) {
        Swal.fire({
          icon: "error",
          title: "Oops...",
          text: "Please,long description min characters",
          footer: "<a href>Why do I have this issue?</a>"
        });
      } else {
        var tagss = tags.split(",");
        if (tagss.length > 4) {
          Swal.fire({
            icon: "error",
            title: "Oops...",
            text: "No more than 4 tags!",
            footer: "<a href>Why do I have this issue?</a>"
          });
          if (
            !owner ||
            !bot ||
            !tags ||
            !library ||
            !short_description ||
            !server_invite ||
            !bot_invite ||
            !prefix ||
            !long_description
          ) {
            Swal.fire({
              icon: "error",
              title: "Oops...",
              text: "Please,fill all inputs",
              footer: "<a href>Why do I have this issue?</a>"
            });
          }
        } else {
          if (!website) {
            $.post(
              "/edit",
              {
                owner: owner,
                bot: bot,
                library: library,
                other_owners: owners,
                background:background,
                prefix: prefix,
                short_description: short_description,
                server_invite: server_invite,
                bot_invite: bot_invite,
                website: "http://www.cloudlist.xyz/",
                long_description: long_description,
                tags: tags
              },
              function(data, status) {
                window.location = data;
              }
            );
          } else {
            $.post(
              "/edit",
              {
                owner: owner,
                bot: bot,
                library: library,
                background:background,
                prefix: prefix,
                other_owners: owners,
                short_description: short_description,
                server_invite: server_invite,
                bot_invite: bot_invite,
                website: website,
                long_description: long_description,
                tags: tags
              },
              function(data, status) {
                window.location = data;
              }
            );
          }
          Swal.fire({
            icon: "success",
            title: "success..."
          });
        }
      }
    })
    .catch(console.error);
}
    </script>
 

  <div class="form-group" style="border-radius:5%px;">
  <button id="addbot" class="btn btn-outline-success" onclick="edit()">
