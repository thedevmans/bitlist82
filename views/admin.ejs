  <div class="tab-content">

   

<div class="container mt-5 body">
  <table class="table table-striped table-hover table-dark">
    <thead>
      <tr>
        <th scope="col">Bot's owner</th>
        <th scope="col">Bot</th>
        <th scope="col">Prefix</th>
        <th scope="col">Invite</th>
        <th scope="col">Actions</th>
      </tr>
    </thead>
    <tbody id="tbody">
      <% data.forEach(function(data) { %>
      <th>
      <input type="text" name="owner" id="inputName" value="<%=data.val().owner%>" class="form-control" readOnly>
      </th>
        <th>
      <input type="text" name="bot" id="inputBot" value="<%=data.val().bot%>" class="form-control" readOnly>
      </th>    
        <th>
      <input type="text"  name="prefix" id="inputPrefix" value="<%=data.val().prefix%>" class="form-control" readOnly>
      </th>   
        <th>
      <button class="btn btn-info" onclick="window.open('<%=data.val().bot_invite%>')">invite</button>      
      </th>  
        <th>
      <button class="btn btn-outline-success" onclick="Accept()">Accept</button>      
      </th>   
        <th>
      <button class="btn btn-outline-danger" onclick="Reject()">Reject</button>      
      </th>
      <% }); %>   
      </tbody>           
  </table>
</div>
 

 

  
  
