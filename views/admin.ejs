    .close {
    background: red;
    width: 25px;
    height: 25px;
    position: relative;
}
.close:after {
    content: '';
    height: 25px;
    border-left: 2px solid #fff;
    position: absolute;
    transform: rotate(45deg);
    left: 12px;
}

.close:before {
    content: '';
    height: 25px;
    border-left: 2px solid #fff;
    position: absolute;
    transform: rotate(-45deg);
    left: 12px;
}

